    "#Q_4 Given a number, print 1 if the number is odd otherwise print 0."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter Number= 5\n",
      "Odd 1\n"
     ]
    }
   ],
   "source": [
    "#program\n",
    "\n",
    "n=int(input('Enter Number= '))\n",
    "\n",
    "while(n%2==0):\n",
    "    print(\"Even\" ,0) #number is even\n",
    "    break\n",
    "    \n",
    "while(n%2!=0):\n",
    "    print(\"Odd\",1) #number is odd\n",
    "    break"
   ]
